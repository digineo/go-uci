config wifi-iface 'guest_radio0'
	option device 'radio0'
	option mode 'ap'
	option network 'guest'
	option isolate '1'
	option ssid 'OpenWrt'
	option disabled 'off'

config wifi-iface 'guest_radio1'
	option device 'radio1'
	option mode 'ap'
	option network 'guest'
	option isolate '1'
	option ssid 'OpenWrt'
	option disabled 'on'
